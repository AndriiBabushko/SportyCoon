{
  "name": "turbo-shad-storybook-starter",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:admin": "turbo run dev --filter ui --filter admin",
    "dev:main": "turbo run dev --filter ui --filter main",
    "dev:both": "turbo run dev --filter ui --filter admin --filter main",
    "dev:storybook": "turbo run dev --filter ui --filter docs",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@turbo/gen": "^1.9.7",
    "eslint": "^7.32.0",
    "eslint-config-custom": "workspace:*",
    "prettier": "^2.5.1",
    "turbo": "latest"
  },
  "packageManager": "pnpm@9.0.4"
}
